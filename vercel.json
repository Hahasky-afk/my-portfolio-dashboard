{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/dashboard/$1"
        }
    ],
    "crons": [
        {
            "path": "/api/update",
            "schedule": "0 * * * *"
        }
    ]
}